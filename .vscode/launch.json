{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "configurations": [
    {
      "name": "Chrome: Attach",
      "port": 9222,
      "request": "attach",
      "type": "pwa-chrome",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "Chrome: Launch",
      "request": "launch",
      "type": "pwa-chrome",
      "url": "http://localhost:8080",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "Edge: Attach",
      "port": 9222,
      "request": "attach",
      "type": "pwa-msedge",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "Edge: Launch",
      "request": "launch",
      "type": "pwa-msedge",
      "url": "http://localhost:8080",
      "webRoot": "${workspaceFolder}"
    },
    {
      "file": "${workspaceFolder}/index.html",
      "name": "Firefox: Launch index.html",
      "reAttach": true,
      "request": "launch",
      "type": "firefox"
    },
    {
      "name": "Firefox: Launch localhost",
      "reAttach": true,
      "request": "launch",
      "type": "firefox",
      "url": "http://localhost/index.html",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "Launch Program",
      "program": "${file}",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "pwa-node"
    },
    {
      "name": "Microsoft Edge: Attach and open the Edge DevTools",
      "request": "attach",
      "type": "vscode-edge-devtools.debug",
      "url": "http://localhost:8080",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "Microsoft Edge: Launch and open the Edge DevTools",
      "request": "launch",
      "type": "vscode-edge-devtools.debug",
      "url": "http://localhost:8080",
      "webRoot": "${workspaceFolder}"
    }
  ],
  "version": "0.2.0"
}
